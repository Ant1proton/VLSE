public class test {
	
	public static void main(String[] args) {
			
		String s = "01111111";
		
		byte[] b = s.getBytes();
		byte[] c = s.getBytes();
		byte[] result = new byte[b.length];
		
		for(int i = 0; i < b.length; i++)
			result[i] = (byte) (b[i] ^c[i]);
		
		
	}

